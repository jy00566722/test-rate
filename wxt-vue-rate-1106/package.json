{
  "name": "wxt-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "devc": "wxt clean && wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "rm -fr ../chrome-mv3 && wxt build && cp -r .output/chrome-mv3 ../chrome-mv3",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.4.4",
    "gulp-javascript-obfuscator": "^1.1.6",
    "pinia": "^2.1.7",
    "vue": "^3.3.13",
    "vue-draggable-plus": "^0.3.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-terser": "^2.1.0",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.26",
    "wxt": "^0.18.1"
  }
